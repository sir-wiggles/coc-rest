{
  "name": "coc-rest",
  "version": "0.0.5-development",
  "description": "A coc-post or vim-rest-console plugin but with more features.",
  "author": "Jeff Oryen <jeffmgreg@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "coc.nvim"
  ],
  "engines": {
    "coc": "^0.0.77"
  },
  "scripts": {
    "lint": "eslint src --ext ts",
    "clean": "rimraf lib",
    "watch": "webpack --watch",
    "build": "webpack",
    "prepare": "npm-run-all clean build"
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 88,
    "semi": true,
    "tabWidth": 4
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "coc.nvim": "^0.0.77",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.2",
    "typescript": "^4.1.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "coc-rest configuration",
      "properties": {
        "coc-rest.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable coc-rest extension"
        },
        "coc-rest.directory": {
          "type": "string",
          "default": "~/.local/share/coc-rest",
          "description": "Directory location where you want your rest files saved."
        },
        "coc-rest.pin-workspace": {
          "type": "string",
          "default": "",
          "description": "The workspace that you want to pin."
        }
      }
    },
    "commands": [
      {
        "command": "coc-rest.Command",
        "title": "coc-rest command title"
      }
    ]
  },
  "dependencies": {
    "axios": "^0.21.0",
    "columnify": "^1.5.4",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.20",
    "log4js": "5.1.0"
  }
}
